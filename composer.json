{
    "name": "enderive/enum",
    "description": "Enum implementation for php below 8.1",
    "authors": [
        {
            "name": "Matthias Szczesny",
            "email": "enderive@outlook.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Enderive\\Enum\\": "src/"
        }
    },
    "require-dev": {
        "php": "^7.4 || ^8.0",
        "phpstan/phpstan": "2.0.x-dev",
        "phpunit/phpunit": "9.6.*",
        "psalm/phar": "dev-master",
        "squizlabs/php_codesniffer": "4.0.x-dev"
    },
    "autoload-dev": {
        "psr-4": {
            "Enderive\\Enum\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "platform-check": false,
        "optimize-autoloader": true
    }
}
